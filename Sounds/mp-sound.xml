<?xml version="1.0" encoding="utf-8"?>
<PropertyList>

<fx>
        <engine-jet-intake-l>
            <name>engine-jet-intake-l</name>
            <path>Aircraft/Supermarine-Swift/Sounds/jet-intake-air.wav</path>
            <mode>looped</mode>
            <position>
                <x>-1.0</x>
                <y>-1.0</y>
                <z>-0.2</z>
            </position>
            <orientation>
                <x>-0.98</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>120</inner-angle>
                <outer-angle>180</outer-angle>
                <outer-gain>0.05</outer-gain>
            </orientation>
            <pitch>
                <factor>0.01</factor>
                <property>engines/engine[0]/n1</property>
            </pitch>
            <volume>
                <factor>0.0333333333333333</factor>
                <property>engines/engine[0]/n2</property>
            </volume>
            <reference-dist>400</reference-dist>
            <max-dist>4000</max-dist>
        </engine-jet-intake-l>

        <engine-jet-intake-r>
            <name>engine-jet-intake-r</name>
            <path>Aircraft/Supermarine-Swift/Sounds/jet-intake-air.wav</path>
            <mode>looped</mode>
            <position>
                <x>-1.0</x>
                <y>1.0</y>
                <z>-0.2</z>
            </position>
            <orientation>
                <x>-0.98</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>120</inner-angle>
                <outer-angle>180</outer-angle>
                <outer-gain>0.05</outer-gain>
            </orientation>
            <pitch>
                <factor>0.01</factor>
                <property>engines/engine[0]/n1</property>
            </pitch>
            <volume>
                <factor>0.0333333333333333</factor>
                <property>engines/engine[0]/n2</property>
            </volume>
            <reference-dist>400</reference-dist>
            <max-dist>4000</max-dist>
        </engine-jet-intake-r>
 
 
        <engine-jet-whine-n1>
            <name>engine-jet-whine-n1</name>
            <path>Aircraft/Supermarine-Swift/Sounds/jet-whine.wav</path>
            <mode>looped</mode>
            <pitch>
                <property>engines/engine[0]/n1</property>
                <ofset>0.2</ofset>
                <factor>0.01</factor>
            </pitch>
            <volume>
                <factor>0.01</factor>
                <property>engines/engine[0]/n2</property>
                <max>0.4</max>
            </volume>
            <position>
                <x>-0.6</x>
                <y>0.0</y>
                <z>-0.37</z>
            </position>
            <orientation>
                <x>0</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>90.0</inner-angle>
                <outer-angle>200.0</outer-angle>
                <outer-gain>0.2</outer-gain>
            </orientation>
            <reference-dist>400</reference-dist>
            <max-dist>3000</max-dist>
        </engine-jet-whine-n1>

        <engine-jet-whine-n21>
            <name>engine-jet-whine-n21</name>
            <path>Aircraft/Supermarine-Swift/Sounds/jet-whine.wav</path>
            <mode>looped</mode>
            <pitch>
                <property>engines/engine[0]/n2</property>
                <ofset>0</ofset>
                <factor>0.01</factor>
            </pitch>
            <volume>
                <factor>0.01</factor>
                <property>engines/engine[0]/n2</property>
                <max>0.4</max>
            </volume>
            <position>
                <x>-0.6</x>
                <y>0.0</y>
                <z>-0.37</z>
            </position>
            <orientation>
                <x>-0.98</x>
                <y>0</y>
                <z>0</z>
                <inner-angle>90.0</inner-angle>
                <outer-angle>200.0</outer-angle>
                <outer-gain>0.2</outer-gain>
            </orientation>
            <reference-dist>400</reference-dist>
            <max-dist>3000</max-dist>
        </engine-jet-whine-n21>
 
 
        <engine-jet-efflux-velocity-blast>
            <name>engine-jet-efflux-velocity-blast</name>
            <path>Aircraft/Supermarine-Swift/Sounds/jet-efflux-2.wav</path>
            <mode>looped</mode>
            <condition>
                <greater-than>
                    <property>engines/engine[0]/thrust_lb</property>
                    <value>200</value>
                </greater-than>
                <greater-than>
                    <property>velocities/airspeed-kt</property>
                    <value>100</value>
                </greater-than>
            </condition>
            <volume>
                <factor>0.4</factor>
            </volume>
            <position>
                <x>3</x>
                <y>0.0</y>
                <z>-0.37</z>
            </position>
            <orientation>
                <x>1.0</x>
                <y>0.0</y>
                <z>0.0</z>
                <inner-angle>68.3</inner-angle>
                <outer-angle>200.0</outer-angle>
                <outer-gain>0.188</outer-gain>
            </orientation>
            <reference-dist>1000</reference-dist>
            <max-dist>12000</max-dist>
        </engine-jet-efflux-velocity-blast>
  
  
        <engine-jet-exhaust>
            <name>engine-jet-exhaust</name>
            <path>Aircraft/Supermarine-Swift/Sounds/engine-jet-exhaust.wav</path>
   <mode>looped</mode>
   <condition>
    	 <not>
                    <property>sim/freeze/engine-sound-freeze</property>
                </not>
                <not>
                    <property>fdm/jsbsim/systems/ecs/internal-and-canopy-closed</property>
     	</not>
   </condition>
            <volume>
                <offset>-0.3</offset>
                <factor>0.01</factor>
                <property>engines/engine[0]/n2</property>
            </volume>
   <position>
                <x>3</x>
	<y>0.0</y>
                <z>-0.37</z>
   </position>
   <orientation>
                <x>1.0</x>
    	<y>0.0</y>
    	<z>0.0</z>
                <inner-angle>68.3</inner-angle>
                <outer-angle>200.0</outer-angle>
                <outer-gain>0.188</outer-gain>
   </orientation>
            <reference-dist>1000</reference-dist>
            <max-dist>9000</max-dist>
        </engine-jet-exhaust>


  
        <engine-jet-afterburner>
            <name>engine-jet-afterburner</name>
            <path>Aircraft/Supermarine-Swift/Sounds/engine-jet-afterburner.wav</path>
   <mode>looped</mode>
   <condition>
     	<not>
                    <property>fdm/jsbsim/systems/ecs/internal-and-canopy-closed</property>
                </not>
                <property>engines/engine[0]/reheat</property>
                <not>
                    <property>sim/freeze/engine-sound-freeze</property>
     	</not>
                <property>engines/engine[0]/running</property>
   </condition>
            <volume>
                <property>engines/engine[0]/reheat</property>
                <factor>0.5</factor>
            </volume>
            <!--
            <volume>
                <offset>0.4</offset>
                <factor>-0.2</factor>
                <property>sim/current-view/internal</property>
            </volume>
-->
   <position>
                <x>3</x>
                <y>1.0</y>
                <z>-0.37</z>
   </position>
   <orientation>
                <x>1.0</x>
    	<y>0.0</y>
    	<z>0.0</z>
                <inner-angle>90.0</inner-angle>
                <outer-angle>200.0</outer-angle>
                <outer-gain>0.15</outer-gain>
   </orientation>
            <reference-dist>900</reference-dist>
            <max-dist>6000</max-dist>
        </engine-jet-afterburner>



        <squeal-nose-gear>
            <name>squeal-nose-gear</name>
            <path>Aircraft/Supermarine-Swift/Sounds/tyre-squeal.wav</path>
            <condition>
                <equals>
                    <property>gear/gear[0]/wow</property>
                    <value>1</value>
                </equals>
                <greater-than>
                    <property>gear/gear[0]/rollspeed-ms</property>
                    <value>30</value>
                </greater-than>
            </condition>
            <position>
                <x>-1.2</x>
                <y>0.0</y>
                <z>-0.6</z>
            </position>
            <pitch>
                <property>velocities/airspeed-kt</property>
                <factor>0.0055</factor>
            </pitch>
            <reference-dist>150</reference-dist>
            <max-dist>500</max-dist>
        </squeal-nose-gear>

        <squeal-left-mlg>
            <name>squeal-left-mlg</name>
            <path>Aircraft/Supermarine-Swift/Sounds/tyre-squeal.wav</path>
            <condition>
                <property>gear/gear[1]/wow</property>
            </condition>
            <reference-dist>150</reference-dist>
            <max-dist>500</max-dist>
            <position>
                <x>1.2</x>
                <y>0.0</y>
                <z>-0.6</z>
            </position>
   <volume>
                <property>velocities/speed-down-fps</property>
                <factor>0.05</factor>
   </volume>
            <pitch>
                <property>velocities/airspeed-kt</property>
                <factor>0.0055</factor>
            </pitch>
        </squeal-left-mlg>
  
        <squeal-right-mlg>
            <name>squeal-right-mlg</name>
            <path>Aircraft/Supermarine-Swift/Sounds/tyre-squeal.wav</path>
            <condition>
                <property>gear/gear[2]/wow</property>
            </condition>
            <position>
                <x>1.2</x>
                <y>1.0</y>
                <z>-0.6</z>
            </position>
            <volume>
                <property>velocities/speed-down-fps</property>
                <factor>0.05</factor>
            </volume>
            <pitch>
                <property>velocities/airspeed-kt</property>
                <factor>0.0055</factor>
            </pitch>
            <reference-dist>150</reference-dist>
            <max-dist>500</max-dist>
        </squeal-right-mlg>

        <squeal-nose-sideslip>
            <path>Aircraft/Supermarine-Swift/Sounds/tyre-squealing-sideslip.wav</path>
            <mode>looped</mode>
			<condition>
                <or>
	    <greater-than>
                        <property>fdm/jsbsim/gear/unit[0]/slip-angle-deg</property>
                        <value>4.5</value>
	    </greater-than>
                    <less-than>
                        <property>fdm/jsbsim/gear/unit[0]/slip-angle-deg</property>
                        <value>-4.5</value>
                    </less-than>
                </or>
				<equals>
                    <property>gear/gear[0]/wow</property>
                    <value>1</value>
				</equals>
                <greater-than>
                    <property>gear/gear[0]/rollspeed-ms</property>
                    <value>1</value>
                </greater-than>
		</condition>
            <position>
                <x>1.2</x>
                <y>1.0</y>
                <z>-0.6</z>
            </position>
            <pitch>
                <factor>0.01</factor>
            </pitch>
			<volume>
                <type>abs</type>
                <factor>0.01</factor>
                <property>fdm/jsbsim/gear/unit[0]/slip-angle-deg</property>
			</volume>
            <reference-dist>150</reference-dist>
            <max-dist>1500</max-dist>
        </squeal-nose-sideslip>

        <squeal-left-mlg-sideslip>
            <name>squeal-left-mlg-sideslip</name>
            <path>Aircraft/Supermarine-Swift/Sounds/tyre-squealing-sideslip.wav</path>
            <mode>looped</mode>
            <condition>
                <or>
                    <greater-than>
                        <property>fdm/jsbsim/gear/unit[1]/slip-angle-deg</property>
                        <value>1.5</value>
                    </greater-than>
                    <less-than>
                        <property>fdm/jsbsim/gear/unit[1]/slip-angle-deg</property>
                        <value>-1.5</value>
                    </less-than>
                </or>
                <greater-than>
                    <property>gear/gear[1]/rollspeed-ms</property>
                    <value>1</value>
                </greater-than>
                <equals>
                    <property>gear/gear[1]/wow</property>
                    <value>1</value>
                </equals>
            </condition>
            <position>
                <x>1.2</x>
                <y>1.0</y>
                <z>-0.6</z>
            </position>
            <volume>
                <type>abs</type>
                <factor>0.01</factor>
                <property>fdm/jsbsim/gear/unit[1]/slip-angle-deg</property>
            </volume>
            <reference-dist>150</reference-dist>
            <max-dist>1500</max-dist>
        </squeal-left-mlg-sideslip>

        <squeal-right-mlg-sideslip>
            <name>squeal-right-mlg-sideslip</name>
            <path>Aircraft/Supermarine-Swift/Sounds/tyre-squealing-sideslip.wav</path>
      <mode>looped</mode>
      <condition> 
                <or>
                    <greater-than>
                        <property>fdm/jsbsim/gear/unit[2]/slip-angle-deg</property>
                        <value>1.5</value>
                    </greater-than>
                    <less-than>
                        <property>fdm/jsbsim/gear/unit[2]/slip-angle-deg</property>
                        <value>-1.5</value>
                    </less-than>
                </or>
                <greater-than>
                    <property>gear/gear[2]/rollspeed-ms</property>
                    <value>1</value>
                </greater-than>
                <equals>
                    <property>gear/gear[2]/wow</property>
                    <value>1</value>
                </equals>
      </condition>
            <position>
                <x>1.2</x>
                <y>1.0</y>
                <z>-0.6</z>
            </position>
      <volume> 
                <type>abs</type>
                <factor>0.01</factor>
                <property>fdm/jsbsim/gear/unit[2]/slip-angle-deg</property>
      </volume>
            <reference-dist>150</reference-dist>
            <max-dist>500</max-dist>
        </squeal-right-mlg-sideslip>

 </fx>


</PropertyList>
