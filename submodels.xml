<!-- Swift FR5 based on F15 submodels; Richard Harrison (rjh@zaretto.com), 2015-02-15 based on F-14 by xii-->

<PropertyList>
    <submodel>
        <name>Belly Tank</name>
        <model>Models/Stores/Ext-Tanks/exttank-submodel.xml</model>
        <trigger>controls/armament/station[2]/jettison-all</trigger>
        <speed>0</speed>
        <repeat>false</repeat>
        <count>1</count>
        <x-offset>0</x-offset>
        <y-offset>0</y-offset>
        <z-offset>0</z-offset>
        <yaw-offset>0</yaw-offset>
        <pitch-offset>-1</pitch-offset>
        <wind>false</wind>
        <eda>7.135</eda> <!--sq ft crosssection-->
        <weight>100</weight> <!--lbs-->
        <cd>0.15</cd> <!--Hoerner, Fluid-Dynamic Drag, 1965, Drag of half bodies on a wall-->
        <contents>consumables/fuel/tank[6]</contents>
        <impact>true</impact>
        <impact-reports>sim/ai/aircraft/impact/droptank</impact-reports>
    </submodel>

	<submodel>
