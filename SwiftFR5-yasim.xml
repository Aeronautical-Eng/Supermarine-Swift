<!-- Internal fuel capacity - 3896 lb  in 3 fuselage + 2 internal wing tanks -->
  <tank x="1.2" y="0.0" z="0.2" jet="true" capacity="862"/>
  <tank x="0" y="0.0" z="0.2" jet="true" capacity="755"/>
  <tank x="-1" y="0.0" z="0.2" jet="true" capacity="801"/>
  <!-- left wing tanks -->
  <tank x="0" y="1.903" z="-0.5" jet="true" capacity="739"/>
  <!-- right wing tanks -->
  <tank x="0" y="-1.903" z="-0.5" jet="true" capacity="739"/>





<ballast x="4.1" y="0" z="0.516" mass="2900" />

 



</airplane>