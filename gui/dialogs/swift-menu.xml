<?xml version="1.0"?>

<PropertyList>

    <default>

        <menu n="50">
            <label>Supermarine Swift FR5</label>
            <enabled type="bool">true</enabled>

            <item>
                <label>Select Livery</label>
                <binding>
                    <command>nasal</command>
                    <script>aircraft.livery.dialog.toggle();</script>
                </binding>
            </item>
            <item>
                <label>Quickstart</label>
                <binding>
                    <command>nasal</command>
                    <script>
                        <![CDATA[ 
                        setprop("controls/engines/engine[0]/cutoff",0);
                        setprop("engines/engine[0]/out-of-fuel",0);
                        setprop("engines/engine[0]/cutoff",0);

                        setprop("fdm/jsbsim/propulsion/starter_cmd",1);
                        setprop("fdm/jsbsim/propulsion/cutoff_cmd",1);
                        setprop("fdm/jsbsim/propulsion/set-running",0);
                    ]]>
                    </script>
                </binding>
            </item>       
               
 

        

            <item>
                <label>Aircraft Options</label>
                <binding>
                    <command>dialog-show</command>
                    <dialog-name>aircraft-dialog</dialog-name>
                </binding>
            </item>

            <!--<item>
                <label>Ground Equipment</label>
                <binding>
                    <command>dialog-show</command>
                    <dialog-name>ground-equipment</dialog-name>
                </binding>
            </item>-->


            <item>
                <label>Registration</label>
                <binding>
                    <command>dialog-show</command>
                    <dialog-name>registration-dialog</dialog-name>
                </binding>
            </item>

            <!--<item>
