 <!-- Panel Lights -->
 
<!-- Red Lights -->
  
    <animation>
  <type>material</type>
  
    <object-name>ab</object-name>
   <object-name>overspeed</object-name>
   <object-name>stall</object-name>
   <object-name>fuel</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.1</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.8</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </emission> 
 </animation>	
	
	 <text>
  <name>ab</name>
  <font>courier_medium.txf</font>
  <character-size type="double">0.05</character-size>
  <offsets>
 
   <x-m>-3.347</x-m>
   <y-m>-0.157</y-m>
   <z-m>0.517</z-m>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">literal</type>
  <text type="string">.</text>
  <draw-text type="bool">true</draw-text>
  <alignment>left-top</alignment>
 </text>
 
 
 <animation>
        <type>select</type>
        <object-name>ab</object-name>
        
        <condition>
       <property>engines/engine[0]/reheat</property>
        </condition>
    </animation>
	
	
	 <text>
  <name>overspeed</name>
  <font>courier_medium.txf</font>
  <character-size type="double">0.05</character-size>
  <offsets>
 
   <x-m>-3.347</x-m>
   <y-m>-0.157</y-m>
   <z-m>0.483</z-m>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">literal</type>
  <text type="string">.</text>
  <draw-text type="bool">true</draw-text>
  <alignment>left-top</alignment>
 </text>
	
<animation>
        <type>select</type>
        <object-name>overspeed</object-name>
        
        <condition>
		<greater-than>
       <property>velocities/groundspeed-kt</property>
	   <value>619.58</value>
	   </greater-than>
        </condition>
    </animation>	
	
	
<text>
  <name>stall</name>
  <font>courier_medium.txf</font>
