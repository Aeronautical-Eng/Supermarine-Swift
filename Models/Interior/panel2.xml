    <interpolation>
      <entry><ind> 0 </ind><dep> -3 </dep></entry>
      <entry><ind> 1 </ind><dep> 85</dep></entry>
    </interpolation>
        <center>
            <x-m>-3.16133</x-m>
            <y-m>-0.350571</y-m>
            <z-m>0.19666</z-m>
        </center>
        <axis>
            <x>0.0</x>
