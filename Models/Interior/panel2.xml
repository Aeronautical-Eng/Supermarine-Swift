<animation>
        <type>select</type>
        <object-name>overspeed</object-name>
        
        <condition>
		<greater-than>
       <property>velocities/groundspeed-kt</property>
	   <!--value>619.58</value-->
	   <value>600</value>
	   </greater-than>
        </condition>
    </animation>	
	
	
<text>
