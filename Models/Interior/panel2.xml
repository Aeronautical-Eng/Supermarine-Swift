  <animation>
    <type>select</type>
    <object-name>wl_cmr_on_on</object-name>
    <object-name>wl_fire_jet_pipe_on</object-name>
    <object-name>wl_fire_engine_on</object-name>
    <object-name>wl_film_out_on</object-name>
    <object-name>wl_cmr_on_on</object-name>
  </animation>
  <animation>
    <type>select</type>
    <object-name>wl_ab_off</object-name>
    <condition>
      <not>
        <greater-than>
          <property>fdm/jsbsim/propulsion/engine[0]/augmentation-alight</property>
          <value>0</value>
        </greater-than>
      </not>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>wl_ab_on</object-name>
    <condition>
      <greater-than>
        <property>fdm/jsbsim/propulsion/engine[0]/augmentation-alight</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>wl_ospd_on</object-name>
    <condition>
      <greater-than>
        <property>velocities/airspeed-kt</property>
        <value>620</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>wl_ospd_off</object-name>
    <condition>
      <not>
        <greater-than>
          <property>velocities/airspeed-kt</property>
          <value>620</value>
        </greater-than>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>wl_stall_on</object-name>
    <condition>
      <and>
        <less-than>
          <property>fdm/jsbsim/gear/wow</property>
          <value>1</value>
        </less-than>
        <greater-than>
          <property>fdm/jsbsim/aero/alpha-deg</property>
          <value>13</value>
        </greater-than>
      </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>wl_stall_off</object-name>
    <condition>
      <not>
        <and>
          <less-than>
            <property>fdm/jsbsim/gear/wow</property>
            <value>1</value>
          </less-than>
          <greater-than>
            <property>fdm/jsbsim/aero/alpha-deg</property>
            <value>13</value>
          </greater-than>
        </and>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>wl_low_fuel_on</object-name>
    <condition>
      <less-than>
        <property>/consumables/fuel/total-fuel-lbs</property>
        <value>1000.00</value>
      </less-than>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>wl_low_fuel_off</object-name>
    <condition>
      <not>
        <less-than>
          <property>/consumables/fuel/total-fuel-lbs</property>
          <value>1000.00</value>
        </less-than>
      </not>
    </condition>
  </animation>

  <!-- wl_hyd_warn warning light -->
  <animation>
    <type>select</type>
    <object-name>wl_hyd_warn_off</object-name>
    <condition>
      <not>
        <or>
          <less-than>
            <property>fdm/jsbsim/systems/hydraulics/system-pressure</property>
            <value>1</value>
          </less-than>
          <less-than>
            <property>fdm/jsbsim/systems/hydraulics/system-pressure1</property>
            <value>1</value>
          </less-than>
        </or>
      </not>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>wl_hyd_warn_on</object-name>
    <condition>
      <or>
        <less-than>
          <property>fdm/jsbsim/systems/hydraulics/system-pressure</property>
          <value>1</value>
        </less-than>
        <less-than>
          <property>fdm/jsbsim/systems/hydraulics/system-pressure1</property>
          <value>1</value>
        </less-than>
      </or>
    </condition>
  </animation>

  <!-- wl_canopy warning light -->
  <animation>
    <type>select</type>
    <object-name>wl_canopy_off</object-name>
    <condition>
      <not>
        <greater-than>
          <property>canopy/position-norm</property>
          <value>0</value>
        </greater-than>
      </not>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>wl_canopy_on</object-name>
    <condition>
      <greater-than>
        <property>canopy/position-norm</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>switch_battery</object-name>
    <property>controls/electric/battery-switch</property>
    <interpolation>
      <entry>
        <ind> 0 </ind>
        <dep> -3 </dep>
      </entry>
      <entry>
        <ind> 1 </ind>
        <dep> 85</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>-3.11033</x-m>
      <y-m>-0.35005</y-m>
      <z-m>0.24839</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
	
	    <animation>
