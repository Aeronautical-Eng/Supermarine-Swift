<!-- Red Lights -->
  
 <animation>
  <type>material</type>
   <object-name>ab</object-name>
   <object-name>overspeed</object-name>
   <object-name>stall</object-name>
   <object-name>fuel</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.1</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.8</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </emission> 
 </animation>	
	
 <text>
  <name>ab</name>
  <font>courier_medium.txf</font>
  <character-size type="double">0.047</character-size>
