<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>oilpressure.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.8 </red>
      <green> 0.7 </green>
      <blue>  0.5 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine[0]/oilp-norm</property>
    <interpolation>
      <entry><ind> 1.21 </ind><dep>   100 </dep></entry>
      <entry><ind> 0.2 </ind><dep>  110 </dep></entry>
      <entry><ind> 0.4 </ind><dep> 120 </dep></entry>
      <entry><ind> 0.6 </ind><dep> 130 </dep></entry>
      <entry><ind> 0.8 </ind><dep> 140 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 170 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


